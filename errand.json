{
    "job": [
        {
            "job_name": "AzCopy Transfer",
            "command_name": "bash",
            "command_args": [ "/home/user/azcopy.sh",
                              "$local_dir",
                              "$action",
                              "$threads",
                              "$protocol",
                              "$proxy",
                              "$container",
                              "$key" ],
            "command_sets": [
                {
                    "variable": "$local_dir",
                    "objects": [ "/home/user/data" ]
                },
                {
                    "variable": "$action",
                    "objects": [ "download" ]
                },
                {
                    "variable": "$threads",
                    "objects": [ "1" ]
                },
                {
                    "variable": "$protocol",
                    "objects": [ "http" ]
                },
                {
                    "variable": "$proxy",
                    "objects": [ "" ]
                },
                {
                    "variable": "$container",
                    "objects": [ "myblob.blob.core.windows.net/container" ]
                },
                {
                    "variable": "$key",
                    "objects": [ "" ]
                }
            ],
            "command_timeout_sec": 45,
            "interval": "*/60 * * * *",
            "max_iterations": 1
        }
    ]
}
