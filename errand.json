{
    "job": [
        {
            "job_name": "Run Sleep Permutation Every 30 Seconds",
            "command_name": "bash",
            "command_args": [ "-c", "sleep $delay" ],
            "command_sets": [
                { "variable": "$delay", "objects": [ "1", "2", "3", "4" ] }
            ],
            "command_timeout_sec": 25,
            "interval": "*/30 * * * * *",
            "max_iterations": 2
        }
    ]
}
