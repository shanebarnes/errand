{
    "job": [
        {
            "job_name": "Awscli http download every hour with 0% packet loss (00:00, 01:00, 02:00, ...)",
            "command_name": "bash",
            "command_args": [
                "/home/user/errand/scripts/object_storage.sh",
                "$output_file",
                "$bin_path",
                "$conf_dir",
                "$local_dir",
                "$action",
                "$threads",
                "$part_size",
                "$loss",
                "$protocol",
                "$proxy",
                "$container",
                "$key",
                "$region"
            ],
            "command_sets": [
                { "variable": "$output_file",    "objects": [ "/home/user/awscli.csv" ] },
                { "variable": "$bin_path",       "objects": [ "/usr/bin/aws" ] },
                { "variable": "$conf_dir",       "objects": [ "/home/user/.aws" ] },
                { "variable": "$local_dir",      "objects": [ "/home/user" ] },
                { "variable": "$action",         "objects": [ "download" ] },
                { "variable": "$threads",        "objects": [ "16" ] },
                { "variable": "$part_size",      "objects": [ "100" ] },
                { "variable": "$loss",           "objects": [ "0.0" ] },
                { "variable": "$protocol",       "objects": [ "http" ] },
                { "variable": "$proxy",          "objects": [ "localhost:8000", "" ] },
                { "variable": "$container",      "objects": [ "mybucket/myprefix" ] },
                { "variable": "$key",            "objects": [ "myaccess", "mysecret" ] },
                { "variable": "$region",         "objects": [ "us-east-1" ] }
            ],
            "command_timeout_sec": 1200,
            "interval": "0 0 */1 * * *",
            "max_iterations": 48
        },
        {
            "job_name": "Awscli https download every hour with 0% packet loss (00:30, 01:30, 02:30, ...)",
            "command_name": "bash",
            "command_args": [
                "/home/user/errand/scripts/object_storage.sh",
                "$output_file",
                "$bin_path",
                "$conf_dir",
                "$local_dir",
                "$action",
                "$threads",
                "$part_size",
                "$loss",
                "$protocol",
                "$proxy",
                "$container",
                "$key",
                "$region"
            ],
            "command_sets": [
                { "variable": "$output_file",    "objects": [ "/home/user/awscli.csv" ] },
                { "variable": "$bin_path",       "objects": [ "/usr/bin/aws" ] },
                { "variable": "$conf_dir",       "objects": [ "/home/user/.aws" ] },
                { "variable": "$local_dir",      "objects": [ "/home/user" ] },
                { "variable": "$action",         "objects": [ "download" ] },
                { "variable": "$threads",        "objects": [ "16" ] },
                { "variable": "$part_size",      "objects": [ "100" ] },
                { "variable": "$loss",           "objects": [ "0.0" ] },
                { "variable": "$protocol",       "objects": [ "https" ] },
                { "variable": "$proxy",          "objects": [ "localhost:8443", "" ] },
                { "variable": "$container",      "objects": [ "mybucket/myprefix" ] },
                { "variable": "$key",            "objects": [ "myaccess", "mysecret" ] },
                { "variable": "$regoin",         "objects": [ "us-east-1" ] }
            ],
            "command_timeout_sec": 1200,
            "interval": "0 */30 */1 * * *",
            "max_iterations": 48
        }
    ]
}
