{
    "job": [
        {
            "job_name": "AzCopy upload",
            "os": "windows",
            "command_name": "powershell",
            "command_args": [
                "C:\\GoPackages\\src\\github.com\\shanebarnes\\errand\\scripts\\Windows\\azcopy.ps1",
                "$output_file",
                "$azcopy_jnl_dir",
                "$local_dir",
                "$action",
                "$threads",
                "$protocol",
                "$proxy",
                "$container",
                "$pattern",
                "$key"
            ],
            "command_sets": [
                { "variable": "$output_file"   , "objects": [ "C:\\AzCopyWorkspace\\azcopy.csv" ] },
                { "variable": "$azcopy_jnl_dir", "objects": [ "C:\\AzCopyWorkspace\\jnl\\out"] },
                { "variable": "$local_dir",      "objects": [ "C:\\AzCopyWorkspace\\out\\data"] },
                { "variable": "$action",         "objects": [ "upload"] },
                { "variable": "$threads",        "objects": [ "64"] },
                { "variable": "$protocol",       "objects": [ "http"] },
                { "variable": "$proxy",          "objects": [ "\"\"", "localhost:8000" ] },
                { "variable": "$container",      "objects": [ "dlalancettewestus.blob.core.windows.net/davidcontainer/data" ] },
                { "variable": "$pattern",        "objects": [ "*.*" ] },
                { "variable": "$key",            "objects": [ "<Key>" ] }
            ],
            "command_timeout_sec": 3600,
            "interval": "0 0,4,8,12,16,20,24,28,32,36,40,44,48,52,56 * * * *",
            "max_iterations": 6
        },
        {
            "job_name": "AzCopy download",
            "os": "windows",
            "command_name": "powershell",
            "command_args": [
                "C:\\GoPackages\\src\\github.com\\shanebarnes\\errand\\scripts\\Windows\\azcopy.ps1",
                "$output_file",
                "$azcopy_jnl_dir",
                "$local_dir",
                "$action",
                "$threads",
                "$protocol",
                "$proxy",
                "$container",
                "$pattern",
                "$key"
            ],
            "command_sets": [
                { "variable": "$output_file"   , "objects": [ "C:\\AzCopyWorkspace\\azcopy.csv" ] },
                { "variable": "$azcopy_jnl_dir", "objects": [ "C:\\AzCopyWorkspace\\jnl\\in" ] },
                { "variable": "$local_dir",      "objects": [ "C:\\AzCopyWorkspace\\in" ] },
                { "variable": "$action",         "objects": [ "download" ] },
                { "variable": "$threads",        "objects": [ "64" ] },
                { "variable": "$protocol",       "objects": [ "http" ] },
                { "variable": "$proxy",          "objects": [ "\"\"", "localhost:8000" ] },
                { "variable": "$container",      "objects": [ "dlalancettewestus.blob.core.windows.net/davidcontainer" ] },
                { "variable": "$pattern",        "objects": [ "data" ] },
                { "variable": "$key",            "objects": [ "<Key>" ] }
            ],
            "command_timeout_sec": 3600,
            "interval": "0 2,6,10,14,18,22,26,30,34,38,42,46,50,54,58 * * * *",
            "max_iterations": 6
        }
    ]
}
