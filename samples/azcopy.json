    "job": [
        {
            "job_name": "AzCopy download every 3 hours with 0% packet loss (01:00, 04:00, 07:00, ...)",
            "command_name": "bash",
            "command_args": [
                "/home/user/errand/scripts/azcopy.sh",
                "$output_file",
                "$azcopy_jnl_dir",
                "$local_dir",
                "$action",
                "$threads",
                "$block_size",
                "$loss",
                "$protocol",
                "$proxy",
                "$container",
                "$key"
            ],
            "command_sets": [
                { "variable": "$output_file"   , "objects": [ "/home/user/azcopy.csv" ] },
                { "variable": "$azcopy_jnl_dir", "objects": [ "/home/user/Microsoft/Azure/AzCopy" ] },
                { "variable": "$local_dir",      "objects": [ "/home/user/data" ] },
                { "variable": "$action",         "objects": [ "download" ] },
                { "variable": "$threads",        "objects": [ "64" ] },
                { "variable": "$block_size",     "objects": [ "4" ] },
                { "variable": "$loss",           "objects": [ "0.0" ] },
                { "variable": "$protocol",       "objects": [ "http" ] },
                { "variable": "$proxy",          "objects": [ "localhost:8000", "" ] },
                { "variable": "$container",      "objects": [ "mystorageaccount.blob.core.windows.net/downloads" ] },
                { "variable": "$key",            "objects": [ "mykey" ] }
            ],
            "command_timeout_sec": 240,
            "interval": "0 0 1/3 * * *",
            "max_iterations": 56
        },
